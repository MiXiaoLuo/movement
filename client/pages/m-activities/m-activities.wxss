/* pages/m-activities/m-activities.wxss */
page{
  background: #f0f0f0;
}
.con-ml{
  width: 100%;
  height: 140rpx;
  background: #fff;
  font-size: 24rpx;
  overflow: hidden;
  margin-top: 20rpx;
}
.con-ml text:first-child{
  margin: 20rpx 0 0 20rpx;
  display: block;
}
.con-ml text:nth-of-type(2){
  display: block;
  text-align: center;
}
.con-ml text:last-child{
  display: block;
  text-align: right;
  margin: 10rpx 20rpx 0 0;
}
