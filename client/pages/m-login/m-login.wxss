/* pages/m-login/m-login.wxss */
page{
  background: #f0f0f0;
}
.loginUser-ml,
.loginPass-ml{
  width: 100%;
  height: 100rpx;
  background: #fff;
}
.loginUser-ml text,
.loginPass-ml text{
  width: 100rpx;
  height: 100rpx;
  font-size: 30rpx;
  display: block;
  float: left;
  line-height: 100rpx;
  margin-left: 100rpx;
}
.loginUser-ml input,
.loginPass-ml input{
  width: 400rpx;
  height: 24rpx;
  font-size: 26rpx;
  border: 2rpx #000 solid;
  float: left;
  padding-left: 20rpx;
  margin: 25rpx;
}
.loginUser-ml{
  position: absolute;
  top: 50%;
  left: 0;
  margin-top: -200rpx;
}
.loginPass-ml{
  position: absolute;
  top: 50%;
  left: 0;
  margin-top: -100rpx;
}
.loginEnroll-ml{
  font-size: 30rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  background: #fff;
  display: block;
  margin-left: -200rpx;
}
.loginBtn-ml{
  width: 550rpx;
  height: 80rpx;
  position: absolute;
  top: 50%;
  left: 105rpx;
  margin-top: 70rpx;
  background: #0391d7;
  color: #fff;
}
