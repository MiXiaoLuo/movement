/* pages/my/my.wxss */
page{
  background: #f0f0f0;
}
.m-nav-ml{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  border-bottom: 5rpx #f0f0f0 solid;
  padding-left: 50rpx;
  font-size: 30rpx;
}
.m-user-ml{
  width: 100%;
  height: 150rpx;
  background: #fff;
  border-bottom: 5rpx #f0f0f0 solid;
  margin-bottom: 20rpx;
  overflow: hidden;
}
.m-user-ml image{
  width: 100rpx;
  height: 100rpx;
  display: block;
  border-radius: 50rpx;
  margin: 25rpx;
  float: left;
}
.m-user-ml text{
  display: block;
  float: left;
  line-height: 150rpx;
  font-size: 40rpx;
}
